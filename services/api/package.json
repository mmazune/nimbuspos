{
  "name": "@chefcloud/api",
  "version": "1.0.0-rc.1",
  "private": false,
  "scripts": {
    "build": "nest build",
    "build:e2e": "tsc -p tsconfig.build.e2e.json",
    "openapi:export": "node scripts/export-openapi.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:setup": "node scripts/setup-test-db.mjs",
    "test:e2e:gate": "node scripts/e2e-gate-runner.mjs",
    "test:e2e:gate:self-check": "node scripts/e2e-gate-runner.mjs --self-check",
    "test:e2e:gate:legacy": "pnpm test:e2e:teardown-check && node scripts/e2e-runtime-matrix.mjs",
    "test:e2e:inventory": "node scripts/inventory-gate-runner.mjs",
    "test:e2e:inventory:self-check": "node scripts/inventory-gate-runner.mjs --self-check",
    "test:e2e:release": "node scripts/release-gate-runner.mjs",
    "test:e2e:release:self-check": "node scripts/release-gate-runner.mjs --self-check",
    "test:e2e:strict": "pnpm test:e2e:teardown-check && node scripts/e2e-strict-runner.mjs",
    "pretest:e2e": "pnpm test:e2e:setup",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:ci": "node scripts/run-e2e-with-deadline.mjs --minutes=25",
    "test:e2e:profile": "node scripts/e2e-profile.mjs",
    "test:e2e:teardown-check": "node scripts/check-e2e-teardown.mjs",
    "test:e2e:coverage-check": "node scripts/check-e2e-coverage.mjs",
    "test:e2e-js": "jest --config ./jest-e2e-js.json",
    "test:e2e-slice": "jest --config ./jest-e2e-slice.json",
    "test:e2e-blackbox": "jest --config ./jest-e2e-blackbox.json",
    "e2e:slice": "jest --config ./jest-e2e-slice.json",
    "e2e:slice:ci": "rimraf ../../reports/junit ../../reports/coverage/e2e-slice && pnpm -s e2e:slice",
    "e2e:slice:view": "xdg-open reports/coverage/e2e-slice/lcov-report/index.html || open reports/coverage/e2e-slice/lcov-report/index.html",
    "smoke:runtime": "bash ../../tools/smoke_runtime.sh",
    "build:api": "pnpm -w -r build",
    "demo:reset:tapas": "npx tsx prisma/reset-tapas-demo.ts"
  },
  "dependencies": {
    "@chefcloud/contracts": "workspace:*",
    "@chefcloud/db": "workspace:*",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^10.3.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.66.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/instrumentation-http": "^0.207.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.54.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@sentry/node": "^10.22.0",
    "@sentry/profiling-node": "^10.22.0",
    "@simplewebauthn/server": "^13.2.2",
    "@simplewebauthn/types": "^12.0.0",
    "argon2": "^0.44.0",
    "bullmq": "^5.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "express": "^4.19.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/argon2": "^0.15.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.23",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.17.3",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "axios": "^1.13.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "madge": "^8.0.0",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.2",
    "rimraf": "^6.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../jest.setup.ts"
    ],
    "verbose": true,
    "reporters": [
      "default"
    ],
    "maxWorkers": "50%"
  }
}