{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globalSetup": "<rootDir>/jest-e2e.setup.ts",
  "setupFilesAfterEnv": ["<rootDir>/handle-debug.setup.ts"],
  "moduleNameMapper": {
    "^@simplewebauthn/server$": "<rootDir>/__mocks__/simplewebauthn.e2e.ts",
    "^@api/(.*)$": "<rootDir>/../src/$1"
  }
}
